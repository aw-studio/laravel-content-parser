        "illuminate/database": "^8.0",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.4",
        "illuminate/testing": "^8.0",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
